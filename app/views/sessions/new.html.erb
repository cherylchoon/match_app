<div class="container">
  <div class="col-md-4">
  </br></br>
    <h4>Sign In</h4>

    <%= form_tag("/sessions", method: "post") do %>
      <div class="form-group">
        <%= label_tag "Email:" %>
        <%= text_field_tag(:email, nil, {class: "form-control"}) %>
      </div>
      <div class="form-group">
        <%= label_tag "Password:" %>
        <%= password_field_tag(:password, nil, {class: "form-control"}) %>
      </div>
      <%= submit_tag "Sign In", class: 'btn btn-outline-info float-right' %>
    <% end %>

    <h6>New to Match.com? <a href="/users/new">Join for FREE</a></h6>

  </div>
</div>
